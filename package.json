{
  "name": "@mysterycode/node-red-sacn",
  "description": "Implementation of sACN (Streaming ACN) for Node-RED",
  "version": "0.0.5",
  "author": "MysteryCode",
  "license": "GPL-3.0-only",
  "private": false,
  "dependencies": {
    "sacn": "^4.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
