    "@types/node-red": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "node-red": "^4.1.0",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
